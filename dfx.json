{
  "canisters": {
    "hello": {
      "build": "sudo ./generate-wasm.sh hello",
      "candid": "backend/src/hello.did",
      "wasm": "target/wasm32-unknown-unknown/release/hello-opt.wasm",
      "type": "custom"
    },
    "hello_assets": {
      "dependencies": [
        "hello"
      ],
      "frontend": {
        "entrypoint": "index.html"
      },
      "source": [
        "src/assets",
        "dist/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.8.1",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 2
}